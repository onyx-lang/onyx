#load "core/module"

use core {*}

overloaded :: #match {
    #order 10 (x: i32) { println("Option X"); },
    #order 5  (y: i32) { println("Option Y"); },
    #order 4  (z: i32) { println("Option Z"); },
}

main :: (args: [] cstr) {
    overloaded(10)
    overloaded(x=10)
    overloaded(r=10)
    overloaded(z=10)
}

#overload #order 3
overloaded :: (q: i32) { println("Option Q"); }

#overload #order 2 
overloaded :: (r: i32) { println("Option R"); }

#overload #order 1
overloaded :: (m: i32) { println("Option M"); }
