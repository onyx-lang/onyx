#load "core/module"

use core
use core {println}

overloaded :: #match {
    (x: str, y: i32) do println("Called str, i32"); ,
    (x: f32, y: str) do println("Called f32, str"); ,
    (x: i32, y: i32) do println("Called i32, i32"); ,
}

main :: (args: [] cstr) {
    x: i32 = 1234
    overloaded(~~x, 4)

    y: i32 = 1234
    overloaded(~~x, "Test")
}
