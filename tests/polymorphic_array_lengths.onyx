

use core {*}

main :: (args: [] cstr) {
    arr := u32.[ 1, 2, 3, 4, 5 ]
    for elem in cast([] u32) arr do printf("{} ", elem)

    roots : [20] f32
    compute_roots(roots)

    for root in roots do println(root)

    compute_roots :: (arr: [] f32) {
        for i in 0 .. arr.count {
            arr[i] = math.sqrt(cast(f32) i)
        }
    }
}
