

use core {*}

Point :: struct { x, y: i32; }

#overload hash.hash :: macro (p: Point) -> _ do return p.x * 1000 + p.y

#operator == macro (p1: Point, p2: Point) -> _ {
    return p1.x == p2.x && p1.y == p2.y
}

main :: (args) => {
    S := Set.make(Point)

    S << Point.{ 1, 2 }
    S << Point.{ 2, 3 }
    S << Point.{ 1, 2 }

    Set.has(&S, Point.{ 1, 2 }) |> println()
}
